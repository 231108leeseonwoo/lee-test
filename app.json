{
  "expo": {
    "name": "LiveScore",
    "slug": "LiveScore",
    "scheme": "LiveScore",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "TestKeyNews" : "bf39bb0eb94c493a9263ffffcda774ed"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ]
  }
}
